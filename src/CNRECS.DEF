*f *e FLOAT_GZ rs_cap17		// ditto 17 F
*f *e FLOAT_GZ rs_COP17
*f *e FRAC rs_CdH;			// heating cycling degradation factor

*f *e FLOAT_GZ rs_inp47		// input power at outdoor dry-bulb = 47 F (Btuh (not W))
