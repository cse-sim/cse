  							// re Ecotope HPWH detailed heat pump model
  *declare "class HPWH* wh_pHPWH;"	// pointer HPWH data else NULL
  *r INT wh_HPWHHSCount;			// # of HPWH heatsources in use for
									//   current config
  *declare "int* wh_HPWHHSMap;"		// runtime map by heat source (allocated at runtime)
									//   0=resistance, 1=compressor
  *s *e *array 2 DBL wh_HPWHUse;	// current step HPWH energy use, kWh
