    ahROLeak=0                  //No return duct leaks
    ahSOLoss=0                  //No supply duct losses
    ahROLoss=0                  //No return duct losses

    //SUPPLY FAN & RETURN FAN
    //==========
    sfanVfDs=1003              //1703.16 m3/hr
